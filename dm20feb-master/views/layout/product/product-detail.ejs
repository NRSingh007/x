    </style>
    <script>
        function showContent(show_tab, hide_tab, show_comp, hide_comp) {
            show_tab = document.getElementById(show_tab);
            hide_tab = document.getElementById(hide_tab);

            show_comp = document.getElementById(show_comp);
            hide_comp = document.getElementById(hide_comp);

            show_tab.classList.add("active");
            hide_tab.classList.remove("active");

            show_comp.style.display = "block";
            hide_comp.style.display = "none";

        }

        function toggleImage(small_id, large_id, prod_img_count) {
            let small_img = document.getElementById(small_id);
            let large_img = document.getElementById(large_id);
            let img_count = parseInt(prod_img_count);

            let smalls = 'product_photo_';
            let large = 'product_photo_lg_';

            for (let i = 0; i < img_count; i++) {
                let cur_small = `${smalls}${i}`
                let cur_large = `${large}${i}`

                if (cur_small == small_id) {
                    document.getElementById(cur_small).classList.add("active");
                } else {
                    document.getElementById(cur_small).classList.remove("active");
                }

                if (cur_large == large_id) {
                    document.getElementById(cur_large).style.display = "block";
                } else {
                    document.getElementById(cur_large).style.display = "none";
                }
            }

        }


        function addToWishlist() {
            const userId = $("#userId").val();
            const productId = $("#productId").val(); 
            return fetch(`/product/wishlist/add/${userId}/${productId}`)
                .then(response => { return response.json() })
                .then(data => {
                    console.log({data});
                    console.log(data.wishlist);
                    if (data.error) {
                        alert('Wishlist already added!');
                    }

                    if (data.wishlist) {
                        alert('Wishlist successfully added!');
                    }
                })
                .catch(err => { console.log(err) });
        }


        function copyLink() {
            document.getElementById('copied_url').value = window.location.href;
            let copyText = document.getElementById("copied_url");
            copyText.focus();
            copyText.select();
            document.execCommand("copy");
            alert("Copied the text: " + copyText.value);
        }
    </script>

