     <div class="modal fade" id="chat_with_seller" tabindex="-1" role="dialog" aria-labelledby="exampleModalCenterTitle"
        aria-hidden="true">
        <div class="modal-dialog modal-dialog-centered" role="document">
            <div class="modal-content">
                <div class="modal-header" style="background-color: #CB202D; color: white;">
                    <h5 class="modal-title" id="exampleModalLongTitle">CHAT WITH SELLER</h5>
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                        <span aria-hidden="true" style="color: white;">&times;</span>
                    </button>
                </div>
                <div class="modal-body">
                    <form action="/product/send/text/<%=product._id%>" method="POST">
                        <div>
                            <span>Quote your price to initiate chat & negotiation</span>
                        </div>
                        <br>
                        <div class="form-group">
                            <input type="text" name="buyer_name" class="form-control" placeholder="Enter your name">
                        </div>
                        <div class="form-group">
                            <input type="email" name="buyer_email" class="form-control" placeholder="Enter your email">
                        </div>
                        <div class="form-group">
                            <input type="email" name="offer_amount" class="form-control" placeholder="Enter your Offer amount">
                        </div>
                        <div class="form-group">
                            <input type="email" name="address" class="form-control" placeholder="Enter your Address">
                        </div>
                        <div class="input-group mb-3">
                            <div class="input-group-prepend">
                                <span class="input-group-text" id="basic-addon1">+91</span>
                            </div>
                            <input type="number" class="form-control" placeholder="Enter your mobile no."
                                name="buyer_phone" aria-label="mobile" aria-describedby="basic-addon1">
                            <input type="hidden" name="product_ad_title" value="<%=product.product_ad_title%>">
                            <input type="hidden" name="seller_email" value="<%=product.email%>">
                        </div>
                        <div>
                            <span>By clicking Make Offer & Chat you will agree to our <a href="https://digitalmanipur.com/site/terms">Terms of Use and <a href="https://digitalmanipur.com/site/privacy">Privacy Policy</a>
                            </span>
                        </div>
                        <br>
                        <div>
                            <button class="btn btn-block custom_post_prod_btn">
                                Make Offer and Chat
                            </button>
                        </div>
                    </form>
                </div>

            </div>
        </div>
    </div>

